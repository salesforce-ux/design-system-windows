<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:Class="Salesforce.DesignSystem.Fonts">

    <FontFamily x:Key="SalesforceDesignSystemIcons">ms-appx:///SalesforceDesignSystem/Generated/SalesforceDesignSystemIcons.ttf#SalesforceDesignSystemIcons</FontFamily>
    <FontFamily x:Key="SalesforceDefaultFontFamily">ms-appx:///SalesforceDesignSystem/Fonts/SalesforceSans-Regular.ttf#Salesforce Sans</FontFamily>
    <FontFamily x:Key="SalesforceSansBoldFontFamily">ms-appx:///SalesforceDesignSystem/Fonts/SalesforceSans-Bold.ttf#Salesforce Sans Bold</FontFamily>

    <FontFamily x:Key="SalesforceSansItalicFontFamily">ms-appx:///SalesforceDesignSystem/Fonts/SalesforceSans-Italic.ttf#Salesforce Sans Italic</FontFamily>
    <FontFamily x:Key="SalesforceLightFontFamily">ms-appx:///SalesforceDesignSystem/Fonts/SalesforceSans-Light.ttf#Salesforce Sans Light</FontFamily>
    <FontFamily x:Key="SalesforceSansLightItalicFontFamily">ms-appx:///SalesforceDesignSystem/Fonts/SalesforceSans-LightItalic.ttf#Salesforce Sans Light Italic</FontFamily>
